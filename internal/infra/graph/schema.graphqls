type Order {
    id: String!
    Price: Float!
    Tax: Float!
    FinalPrice: Float!
}

input OrderInput {
    id : String!
    Price: Float!
    Tax: Float!
}

type Mutation {
    createOrder(input: OrderInput): Order
}

type Query {
    listOrders: [Order!]!
}

# Para gerar o schema.graphqls
# go run github.com/99designs/gqlgen init 

# Para atualizar o schema.graphqls
# go run github.com/99designs/gqlgen generate